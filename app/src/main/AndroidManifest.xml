<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.cynthia.relax">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".activitis.login.LoginActivity">
        </activity>
        <activity android:name=".activitis.main.MainActivity">
        </activity>
        <activity android:name=".activitis.register.RegisterActivity">
        </activity>
        <activity android:name=".activitis.historyorder.HistoryOrderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".activitis.orderdetail.OrderDetailActivity"
                android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activitis.comment.CommentActivity">
        </activity>
        <activity android:name=".activitis.order.OrderActivity">

        </activity>
    </application>

</manifest>